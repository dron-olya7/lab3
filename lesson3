public class lesson3 {

	public static void main(String[] args) {
	
			Scanner in = new Scanner(System.in); 
			System.out.print("Введите число:");
			int n = in.nextInt();
			int a = 1;
			int b = 1;
			int c;
			 
			System.out.print(a+" "+b+" ");
			for(int i = 2; i <= n; i++){
				c = a + b;
				System.out.print(c+" ");
				a=b;
				b=c;
			}
			System.out.println();
		}
	}
